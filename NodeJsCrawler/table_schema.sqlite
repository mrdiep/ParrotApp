CREATE TABLE "songMetadata" (
    "id" INTEGER PRIMARY KEY NOT NULL,
    "title" TEXT,
    "rhythm" TEXT,
    "chord" TEXT,
    "author" TEXT,
    "singer" TEXT
);


CREATE TABLE "versions" (
    "id" INTEGER PRIMARY KEY,
    "songId" INTEGER,
    "chord" TEXT,
    "content" TEXT,
    "description" TEXT,
    "star" TEXT,
    "votes" TEXT,
    "updated" TEXT,
    "default" INTEGER    
);